<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>loginController</servlet-name>
  	<display-name>loginController</display-name>
  	<description></description>
  	<servlet-class>Controller.loginController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UsernameCheckController</servlet-name>
  	<display-name>UsernameCheckController</display-name>
  	<description></description>
  	<servlet-class>Controller.UsernameCheckController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>addMemberController</servlet-name>
  	<display-name>addMemberController</display-name>
  	<description></description>
  	<servlet-class>Controller.addMemberController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>loginController</servlet-name>
  	<url-pattern>/loginController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UsernameCheckController</servlet-name>
  	<url-pattern>/UsernameCheckController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addMemberController</servlet-name>
  	<url-pattern>/addMemberController</url-pattern>
  </servlet-mapping>
</web-app>
